#include <iostream>
using namespace std;

int main()
{
  int m;

  int n;
  cin >> m;
  int result[m];
  for(int j=0;j<m;j++)
  {
    result[j]=0;
    cin >> n;
    int num[n];
    for(int i=0;i<n;i++)
    {
      cin >> num[i];
    }

    for(int i=0;i<n;i++)
    {
      result[j] = result[j] + num[i];
    }

  }
  for(int i=0;i<m;i++)
  {
    cout << result[i]<<"\n";
  }

}
